{
    "presets": [
        [
            "@babel/env", {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "ie >= 11"
                    ]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime", {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "@babel/runtime"
            }
        ]
    ]
}
